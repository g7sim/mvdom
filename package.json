{
  "name": "mvdom",
  "version": "0.8.8",
  "description": "DOM Centric MVC micro library (zero dependency, < 12kb min, < 5kb gzip)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mvdom/mvdom.git"
  },
  "keywords": [
    "html5",
    "DOM",
    "DOM Centric",
    "Patterns",
    "html",
    "framework",
    "mvc",
    "web-framework",
    "handlebars"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^16.5.0",
    "chokidar": "^3.0.2",
    "cmdrouter": "^0.3.1",
    "exorcist": "^1.0.1",
    "fs-extra-plus": "^0.5.9",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "terser": "^4.2.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "scripts": {
    "prepare": "rm -Rf ./dist && tsc",
    "test": "echo \"Start server here, and test via browser http://localhost:8888/test \" && exit 0",
    "build": "./node_modules/.bin/ts-node --files --project ./scripts/tsconfig.json  ./scripts/src/cmds.ts build",
    "watch": "./node_modules/.bin/ts-node --files --project ./scripts/tsconfig.json  ./scripts/src/cmds.ts watch"
  },
  "author": "Jeremy Chone <jeremy.chone@gmail.com> (http://jeremychone.com)",
  "license": "MIT"
}
